    <script>
        function runfunction(navSource, navAction) {
            var koRegisterCreate = function () {
                $("#registerMain").show();
            };
            switch (navSource) {
            case "Register":
            {
                switch (navAction) {
                case "Create":
                {
                    koRegisterCreate();
                }

                default:
                }
            }

            default:
            }
            alert("Nav Source: " + navSource + "  Nav Action: " + navAction);
        };      
    </script>
}
